{
  "id": "ee0f4287",
  "name": "Nano Banana",
  "version": "1.0.0",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "25.0.0"
  },
  "manifestVersion": 5,
  "requiredPermissions": {
    "launchProcess": {
      "schemes": [
        "https"
      ],
      "extensions": [
        ".jpg"
      ]
    },
    "localFileSystem": "request",
    "network": {
      "domains": [
        "https://generativelanguage.googleapis.com",
        "http://localhost:3000"
      ]
    }
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "vanilla",
      "minimumSize": {
        "width": 230,
        "height": 230
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 300
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/dark@2x.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/dark@2x.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ],
      "label": {
        "default": "Nano-Banana"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/pluginIcon.png",
      "scale": [
        1,
        1.25,
        1.5,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "light",
        "lightest",
        "all"
      ]
    }
  ]
}